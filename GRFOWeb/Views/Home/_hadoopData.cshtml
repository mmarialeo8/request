@model APVDataModels


@{
    var enabledDisabled = string.Empty;
    if (Model == null)
    {
        enabledDisabled = string.Empty;
    }
    else
    {
        enabledDisabled = Model.enabledDisabledStatus;
    }
}

<!-- Show the cropped image in modal -->
<div class="">

    <div class="row">


        <table id="dtRequest" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Part Description</th>
                    <th>STD Cost Base Part</th>
                    <th>Annual Repair Forecast</th>
                    <th>Extended Spend Potential</th>
                    <th>T1 Customer Qty</th>
                    <th>T2 Customer Qty</th>
                    <th>MG3</th>
                    <th>PBG</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        @(Model != null ? Model.partDescription : "")
                    </td>
                    <td>
                        @(Model != null ? Model.stdCostBasePart : "")
                    </td>
                    <td>
                        @(Model != null ? Model.annualRepairForecast : "")
                    </td>
                    <td>
                        @(Model != null ? Model.extendedSpendPotential : "")
                    </td>
                    <td>
                        @(Model != null ? Model.t1CustomerQty : "")
                    </td>
                    <td>
                        @(Model != null ? Model.t2CustomerQty : "")
                    </td>
                    <td>
                        @(Model != null ? Model.mg3 : "")
                    </td>
                    <td>
                        @(Model != null ? Model.pbg : "")
                    </td>
                </tr>
            </tbody>
        </table>



